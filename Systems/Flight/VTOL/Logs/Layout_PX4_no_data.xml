  <snippet name="alt_gps">
   <global>is_first = true
first_value = 0</global>
   <function>if (is_first) then
  is_first = false
  first_value = value
end

return (value - first_value)/1000</function>
   <linked_source>vehicle_gps_position/alt</linked_source>
  </snippet>
