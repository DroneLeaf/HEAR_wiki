  <snippet name="norm_derivative_pos">
   <global></global>
   <function>return (value^2+v1^2+v2^2)^0.5</function>
   <linked_source>derivative_x</linked_source>
   <additional_sources>
    <v1>derivative_y</v1>
    <v2>derivative_z</v2>
   </additional_sources>
  </snippet>
  <snippet name="derivative_z">
   <global>firstX = 0
firstY = 0
is_first = true
secondX = 0
secondY = 0
is_second = false</global>
   <function>-- Wait for initial values
if (is_first) then
  is_first = false
  is_second = true
  firstX = time
  firstY = value
end

if (is_second) then
  is_second = false
  secondX = time
  secondY = value
end

-- Central derivative: dy/dx ~= f(x+delta_x)-f(x-delta_x)/(2*delta_x)
dx = time - firstX
dy = value - firstY
-- Increment
firstX = secondX
firstY = secondY
secondX = time
secondY = value

return dy/dx</function>
